*{
    box-sizing: border-box;
    margin:0;
    padding:0;
}
*:before,*:after{
    box-sizing: border-box;
}
ul,ol{
    list-style:none;
}
/*style*/
body{
    border:1px solid red;
}
.globalHeader{
    /* border:1px solid red; */
    margin:20px;
    display:flex;
    justify-content: space-between;
}
.globalHeader > input{
    width:100%;
    height:40px;
    margin-right:10px;
    padding:0 10px;
    border:none;
    border:1px solid #ddd;
    border-radius:4px;

}
.globalHeader > button{
    white-space:nowrap;
    padding:0 10px;
    border:none;
    border:1px solid #ddd;
    border-radius:4px;
}
